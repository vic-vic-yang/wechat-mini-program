<!--component/picker/one.wxml-->
<view wx:if="{{showChoose}}" class='choose-view main-col' >
    <view class='cancel' bindtap='showModel'>
    
    </view>
    
    <view class='chooseSure ' >
      <view class='datesure main-s28 main-c99' style='background-color:#F7F7FA'  bindtap='showModel'>取消</view>
      <view style='flex:1'></view>
      <view class='datesure main-s28 main-white' bindtap='dataSure'>确定</view>
    </view>
    <row-line/>
    <picker-view indicator-style="height: 50px;" style="width: 100%; height: 300rpx; background-color:#fff;" value="{{vauleIndex}}" bindchange="bindChange">
      <picker-view-column>
        <view wx:for="{{showData}}" wx:key="{{item}}" style="line-height: 50rpx;font-size:28rpx;" class='main-center'>{{item}}</view>
      </picker-view-column>
     
     
    </picker-view>

  </view>
